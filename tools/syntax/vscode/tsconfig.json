{
    "compilerOptions": {
        "esModuleInterop": true,
        "module": "node16",
        "moduleResolution": "node16",
        "target": "es2022",
        "outDir": "out",
        "lib": [
            "ES2023"
        ],
        "sourceMap": true,
        "rootDir": ".",
        "alwaysStrict": true,
        "allowJs": true,
        "strictNullChecks": true,
        "strict": true
    },
    "exclude": [
        "node_modules",
        "out"
    ]
}
